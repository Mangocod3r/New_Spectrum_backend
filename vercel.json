{
    "version":2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "api/*": {
          "memory": 512,
          "timeout":30
        }
      }
}
